    "main": "dist/index.js",
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup",
        "postinstall": "esno ./script/preinstall.dev"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "resolutions": {
        "@langchain/core": "0.1.5"
    },
    "devDependencies": {
        "@langchain/openai": "^0.2.1",
        "@types/fs-extra": "^11.0.4",
        "@types/global-agent": "^2.1.3",
        "@types/shelljs": "^0.8.15",
        "@types/vscode": "^1.91.0",
        "esno": "^4.7.0",
        "fs-extra": "^11.2.0",
        "get-proxy-settings": "^0.1.13",
        "global-agent": "^3.0.0",
        "langchain": "^0.2.8",
        "log4js": "^6.9.1",
        "radash": "^12.1.0",
        "shelljs": "^0.8.5",
        "tsup": "^8.1.0",
        "typescript": "^5.5.3",
        "undici": "^6.19.2",
        "vite": "^5.3.3",
        "vitest": "^1.6.0"
    },
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "contributes": {
        "commands": [
            {
                "command": "dys.showDialog",
                "title": "测试一下"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "dys.showDialog",
                    "group": "0_navigation@0"
                }
            ],
            "editor/context": [
                {
                    "when": "editorFocus",
                    "command": "dys.showDialog",
                    "group": "0_navigation@0"
                }
            ]
        },
        "configuration": {
            "id": "gpt-simple",
            "title": "gpt-simple",
            "properties": {
                "321321.apiKey": {
                    "type": "string",
                    "description": "API Key",
                    "default": ""
                },
                "gpt-simple.model": {
                    "enumItemLabels": [
                        "openAI gpt-4",
                        "openAI gpt-4-32k",
                        "openAI gpt-3.5-turbo",
                        "openAI gpt-3.5-turbo-16k"
                    ],
                    "type": "string",
                    "enum": [
                        "gpt-4",
                        "gpt-4-32k",
                        "gpt-3.5-turbo",
                        "gpt-3.5-turbo-16k"
                    ],
                    "description": "Model",
                    "default": "gpt-3.5-turbo"
                },
                "gpt-simple.baseurl": {
                    "type": "string",
                    "description": "Model",
                    "default": "https://api.openai.com/v1"
                }
            }
        }
    }
}
